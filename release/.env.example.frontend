# EduGrowHub Frontend Environment Variables
# Copy this file to .env.local and update with your actual values

# API Configuration
REACT_APP_API_BASE_URL=https://api.edugrowhub.com
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_JWT_SECRET_KEY=your-jwt-secret-key-here
REACT_APP_SESSION_TIMEOUT=3600000

# External Services
REACT_APP_GOOGLE_CLIENT_ID=your-google-oauth-client-id
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_STRIPE_PUBLIC_KEY=pk_live_your-stripe-public-key

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET_NAME=edugrowhub-assets
REACT_APP_CLOUDFRONT_DOMAIN=cdn.edugrowhub.com

# Analytics & Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX-X
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_VIDEO_CALLS=true
REACT_APP_ENABLE_PAYMENT_INTEGRATION=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Environment
REACT_APP_NODE_ENV=production
REACT_APP_DEBUG_MODE=false

# Application Settings
REACT_APP_APP_NAME=EduGrowHub
REACT_APP_APP_VERSION=1.0.0
REACT_APP_SUPPORT_EMAIL=support@edugrowhub.com
REACT_APP_CONTACT_PHONE=+1-555-0123

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=pdf,doc,docx,ppt,pptx,jpg,jpeg,png,gif

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_ENABLE_SERVICE_WORKER=true

# Localization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr,de

# Social Media Links
REACT_APP_FACEBOOK_URL=https://facebook.com/edugrowhub
REACT_APP_TWITTER_URL=https://twitter.com/edugrowhub
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/edugrowhub
REACT_APP_INSTAGRAM_URL=https://instagram.com/edugrowhub
