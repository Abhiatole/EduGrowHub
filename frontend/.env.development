# EduGrowHub Frontend Environment Configuration
# Development Environment

# API Configuration
REACT_APP_API_URL=http://localhost:8080/api
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_NAME=EduGrowHub
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Features Flags
REACT_APP_ENABLE_REGISTRATION=true
REACT_APP_ENABLE_WHATSAPP_NOTIFICATIONS=true

# Security Configuration
REACT_APP_JWT_STORAGE_KEY=edugrowhub_token
REACT_APP_REMEMBER_ME_DAYS=7

# UI Configuration
REACT_APP_THEME_PRIMARY=#3B82F6
REACT_APP_THEME_SECONDARY=#10B981
REACT_APP_THEME_ACCENT=#F59E0B

# Analytics (if needed)
REACT_APP_ENABLE_ANALYTICS=false
